
pipeline{
    agent any
    tools{
        jdk 'myJava'
        maven 'mymaven'
    }
    stages{
        stage('COMPILE'){
            steps{
                script{
                    echo "compiling the code"
                    git 'https://github.com/Sravanthi2030/COPADO.git'
                    sh 'mvn compile'
                }
            }
        }
        stage('UNITTEST'){
            steps{
                script{
                    echo "Running The Test Cases"
                    sh 'mvn test'
                }
            }
        }
        stage('PACKAGE'){
            steps{
                script{
                    echo "Packaging the code"
                    sh 'mvn package'
                }
            }
        }
       
    }

}
